<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <SolidColorBrush x:Key="TopBarColor" Color="#FBEDFF" />
    <SolidColorBrush x:Key="DarkGrey" Color="#E4CDEB" />
    <SolidColorBrush x:Key="MainFontColor" Color="Black" />
    <SolidColorBrush x:Key="TextBoxMainColor" Color="White" />
    <SolidColorBrush x:Key="MenuButtonColor" Color="#E27EFF" />
    <SolidColorBrush x:Key="DarkBackground" Color="#F5F5F5" />
    <SolidColorBrush x:Key="RowHoverBrushDark" Color="#E0E0E0" />
    <SolidColorBrush x:Key="RowSelectedBrushDark" Color="#BBDEFB" />
    <SolidColorBrush x:Key="HeaderBrushDark" Color="#E1E1E1" />
    <SolidColorBrush x:Key="TextBrushDark" Color="#212121" />
    <SolidColorBrush x:Key="AlternateRowDark" Color="#F0F0F0" />
    <SolidColorBrush x:Key="RowBackgroundDark" Color="#FFFFFF" />
    <SolidColorBrush x:Key="BorderDark" Color="#CCCCCC" />
    <SolidColorBrush x:Key="ButtonBackgroundDark" Color="#2196F3" />
    <SolidColorBrush x:Key="ButtonDisabledDark" Color="#BBDEFB" />
    <SolidColorBrush x:Key="ButtonTextDark" Color="White" />
    <SolidColorBrush x:Key="ButtonDisabledTextDark" Color="#757575" />
    <Style x:Key="ThemeIMG" TargetType="Image">
        <Setter Property="Source" Value="/img/Sun.png" />
    </Style>
    <Style TargetType="Image">
        <Setter Property="RenderOptions.BitmapScalingMode" Value="HighQuality" />
    </Style>
    <Style x:Key="ExpanderIMG" TargetType="Image">
        <Setter Property="Source" Value="/img/arrow_black.png" />
    </Style>
    <Style x:Key="SearchIMG" TargetType="Image">
        <Setter Property="Source" Value="/img/search_black.png" />
    </Style>
    <Style x:Key="AddGroupIMG" TargetType="Image">
        <Setter Property="Source" Value="/img/plus_black.png" />
    </Style>
    <Style x:Key="AddToGroupIMG" TargetType="Image">
        <Setter Property="Source" Value="/img/plus_small_black.png" />
    </Style>
    <Style x:Key="StartSnakeIMG" TargetType="Image">
        <Setter Property="Source" Value="/img/play.png" />
    </Style>
</ResourceDictionary>