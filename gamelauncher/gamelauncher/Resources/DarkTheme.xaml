<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <SolidColorBrush x:Key="TopBarColor" Color="#1E1E1E" />
    <SolidColorBrush x:Key="DarkGrey" Color="#181818" />
    <SolidColorBrush x:Key="MainFontColor" Color="White" />
    <SolidColorBrush x:Key="TextBoxMainColor" Color="#252525" />
    <SolidColorBrush x:Key="MenuButtonColor" Color="#3E3E3E" />
    <SolidColorBrush x:Key="DarkBackground" Color="#1E1E1E" />
    <SolidColorBrush x:Key="RowHoverBrushDark" Color="#2A2A2A" />
    <SolidColorBrush x:Key="RowSelectedBrushDark" Color="#3A3D41" />
    <SolidColorBrush x:Key="HeaderBrushDark" Color="#333" />
    <SolidColorBrush x:Key="TextBrushDark" Color="#FFFFFF" />
    <SolidColorBrush x:Key="AlternateRowDark" Color="#262626" />
    <SolidColorBrush x:Key="RowBackgroundDark" Color="#222" />
    <SolidColorBrush x:Key="BorderDark" Color="#444" />
    <SolidColorBrush x:Key="ButtonBackgroundDark" Color="#DDDDDD" />
    <SolidColorBrush x:Key="ButtonDisabledDark" Color="LightGray" />
    <SolidColorBrush x:Key="ButtonTextDark" Color="Black" />
    <SolidColorBrush x:Key="ButtonDisabledTextDark" Color="DarkGray" />
    <Style x:Key="ThemeIMG" TargetType="Image">
        <Setter Property="Source" Value="/img/Moon.png" />
    </Style>
    <Style TargetType="Image">
        <Setter Property="RenderOptions.BitmapScalingMode" Value="HighQuality" />
    </Style>
    <Style x:Key="ExpanderIMG" TargetType="Image">
        <Setter Property="Source" Value="/img/arrow_white.png" />
    </Style>
    <Style x:Key="SearchIMG" TargetType="Image">
        <Setter Property="Source" Value="/img/search.png" />
    </Style>
    <Style x:Key="AddGroupIMG" TargetType="Image">
        <Setter Property="Source" Value="/img/plus.png" />
    </Style>
    <Style x:Key="AddToGroupIMG" TargetType="Image">
        <Setter Property="Source" Value="/img/plus_small.png" />
    </Style>
    <Style x:Key="StartSnakeIMG" TargetType="Image">
        <Setter Property="Source" Value="/img/play_light.png" />
    </Style>
</ResourceDictionary>